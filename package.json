{
    "name": "vite-plugin-emscripten",
    "version": "1.0.0",
    "description": "A Vite plugin for compiling Emscripten projects",
    "author": {
        "name": "Blaquewithaq",
        "email": "aureavisio@gmail.com"
    },
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/aureavisio/vite-plugin-emscripten/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/aureavisio/vite-plugin-emscripten.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "type": "module",
    "files": ["dist/**/*"],
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "format": "bunx @biomejs/biome format . --write",
        "lint": "bunx @biomejs/biome lint .",
        "check": "bunx @biomejs/biome check --write .",
        "dev": "tsup --watch",
        "builder": "tsup",
        "publisher": "bun builder && changelogen --release --push && bun publish --access public"
    },
    "dependencies": {
        "lodash-es": "^4.17.21",
        "terser": "^5.37.0",
        "vite-plugin-dts": "^4.5.0",
        "vite-plugin-static-copy": "^2.2.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@types/bun": "^1.2.2",
        "@types/lodash-es": "^4.17.12",
        "changelogen": "^0.5.7",
        "tsup": "^8.3.6",
        "vite": "^6.0.11"
    },
    "peerDependencies": {
        "typescript": "5.7.3"
    },
    "trustedDependencies": ["@biomejs/biome"]
}
